     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true">
     <size width="608" height="500" />
    </window>
   </settings>
   <checkButton />
   <document>
    <style name="targetError" base="target" foreground="0xFF0000" />
    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="default" family="Arial" background="0xFFFFFF" tabWidth="12" size="11" bold="false" italic="false" />
    <section>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify an efficient method of storing kitchen items such as pots, pans, and mixing bowls.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  largest items in largest cupboards</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  bulky items in pantry or closet</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  higher in cupboards based on more frequent use</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  store things close to where you use them</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">What is an ideal method for de-cluttering the kitchen?</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  yearly, empty all cupboards and re-assess each item</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  yearly, remove unnecessary duplicates, things no longer   used, damaged items beyond repair</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  monthly, remove unnecessary duplicates, things no longer used, damaged items beyond repair</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  yearly, hire a home organizer to re-evaluate kitchen organization</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify the best type of container for storage of both large and small items throughout the home.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  see through containers with easy to read labels</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">() color coded containers for each section of the home</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  see through containers with numbered labels corresponding to master list</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">() cardboard containers that are easy to label</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify the two items ideal for organizing drawers  throughout the home.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  sturdy adjustable dividers and cloth liners</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  large sized dividers and wallpaper liners</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  multiple sized sturdy adjustable dividers and non-skid liners</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  small sized sturdy dividers and scented liners</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify a practical and inexpensive way to organize and maximize closet space.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  have customized metal shelving installed</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  have customized wood shelving installed</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  add DIY storage containers or cubbies at the base and top of a closet</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  use large storage bins in each closet</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify an ideal method of organizing household papers.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  create a home information center where all paperwork is sorted and filed</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  keep a filing system in the kitchen, living area and main bedroom</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  keep all paperwork in small color coded containers close to a home office space</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  keep all paperwork in a large container in a storage area</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify an efficient and inexpensive sorting method to begin household organization.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  hire a home organizer</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  label two boxes: keep, store</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  label four boxes: trash, give away, keep, relocate</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">() install storage areas in each major room of the house</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">Identify a way to easily organize a kitchen pantry.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  transfer most foods to coded containers</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  install sliding shelves for easy access</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  organize in food zones, use small containers for small items</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  organize foods according to size, use small containers for small items</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">What is a drop zone in an entryway?</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  carpet area for shoes</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  space to hang coats and bags</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  large container in closet space for a variety of items</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>()</target>
      <text family="Arial" background="0xFFFFFF" size="22">  space designated for mail, papers, and keys</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">To reduce clutter in a linen closet, what is the maximum amount of sheet sets that should be kept for each </text>
      <text family="Arial" background="0xFFFFFF" size="22">bed?</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  1</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  2</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  3</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="22">()  4</text>
     </p>
    </section>
   </document>
