   <checkButton />
   <document>
    <style name="targetError" base="target" foreground="0xFF0000" />
    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="default" family="Arial" background="0xFFFFFF" tabWidth="12" size="11" bold="false" italic="false" />
    <section>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">Wash care symbols are special instructions to help you care for your clothing.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>True</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">False</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">A machine wash symbol with one dot means the garment should be washed in hot water.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">True</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>False</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">An X over the machine wash symbol means the garment should be dry cleaned.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>True</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">False</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">The square symbol represents whether or not you can use bleach on the fabric.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">True</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>False</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">The tumble dry symbol is a square with a circle inside.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">True</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>False</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">Three dots on the dryer symbol means you can use high temperature to dry the fabric.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>True</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">False</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">The dry clean symbol is represented by a circle.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>True</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">False</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">An X over the bleach symbol means to cautiously use chlorine bleach.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">True</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <target>False</target>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">One dot on the tumble dry symbol means to use low temperature to dry the fabric.</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">True</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12">
      <text family="Arial" background="0xFFFFFF" size="12">False</text>
     </p>
     <p name="default" family="Arial" background="0xFFFFFF" tabWidth="12" />
    </section>
   </document>
