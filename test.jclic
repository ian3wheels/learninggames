  <activity class="@text.FillInBlanks" name="Wash Care Symbols" code="">
   <description />
   <messages>
    <cell type="initial">
     <style />
     <p>Wash Care Symbols Pick True or False</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <container bgColor="0xFFFFFF">
     <counters time="false" actions="false" score="true" />
    </container>
    <window bgColor="0xFFFFFF" border="true">
     <position x="300" y="0" />
     <size width="900" height="999" />
    </window>
    <eventSounds enabled="false" />
    <skin file="@blue.xml" />
   </settings>
   <document>
    <style name="targetError" base="target" foreground="0xFF0000" />
    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="default" family="Arial" background="0xFFFFFF" size="11" bold="false" italic="false" tabWidth="12" />
    <section>
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16">Wash care symbols are special instructions to help you care for your clothing. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option> True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">A machine wash symbol with one dot means the garment should be washed in hot water.  </text>
      <target>
       <answer>False</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">An X over the machine wash symbol means the garment should be dry cleaned. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">The square symbol represents whether or not you can use bleach on the fabric. </text>
      <target>
       <answer>False</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">The tumble dry symbol is a square with a circle inside. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">Three dots on the dryer symbol means you can use high temperature to dry the fabric. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">The dry clean symbol is represented by a circle. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">An X over the bleach symbol means to cautiously use chlorine bleach. </text>
      <target>
       <answer>False</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">One dot on the tumble dry symbol means to use low temperature to dry the fabric. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="16" bold="false" italic="false">Without a wash care symbol attached to a garment, refer to care based on type of fabric. </text>
      <target>
       <answer>True</answer>
       <optionList>
        <option>True</option>
        <option>False</option>
       </optionList>
       <text>True</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF">               </text>
     </p>
    </section>
   </document>
   <evaluator class="@ComplexEvaluator" />
  </activity>
