  <activity class="@text.FillInBlanks" name="Canadian Food of Guide Quiz" code="">
   <description />
   <messages>
    <cell type="initial">
     <style />
     <p>Please pick the right answer</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <container bgColor="0xFFFFFF">
     <counters time="false" actions="false" score="true" />
    </container>
    <window bgColor="0xFFFFFF" border="true">
     <size width="800" height="999" />
    </window>
