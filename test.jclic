    <skin file="@blue.xml" />
   </settings>
   <document>
    <style name="targetError" base="target" foreground="0xFF0000" />
    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="default" family="Arial" background="0xFFFFFF" size="11" bold="false" italic="false" tabWidth="12" />
    <section>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">The new Canadian Food Guide recommends avoiding trans fats in the diet. Trans fats are:  </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">Purchasing whole wheat bread is a guarantee that the bread is made from whole grain fibre. </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">The amount of fibre grams adults should consume daily is: </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">A body mass index (BMI) of 25 or higher is a healthy range for men and women. </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">Lycopene is: </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">The omega-3 fat source found in omega-3 eggs is usually derived from; </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">One of the best ways to boost immune system function is to add a little garlic to your recipes. </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">When purchasing a whey protein powder, powders that contain higher amounts of protein include: </text>
      <target>
       <answer>Polyunsaturated fats</answer>
       <optionList>
        <option>Monounsaturated fats</option>
        <option>Polyunsaturated fats</option>
        <option>Saturated fats</option>
        <option>Trans fatty acids</option>
       </optionList>
       <text>Answer</text>
      </target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">Omega-3 fats are: </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
     <p family="Arial" background="0xFFFFFF">
      <text family="Arial" background="0xFFFFFF" size="14">HDL is the "good" cholesterol and necessary for optimal health. </text>
      <target>Answer</target>
     </p>
     <p family="Arial" background="0xFFFFFF" />
    </section>
   </document>
   <evaluator class="@ComplexEvaluator" />
  </activity>
  <activity class="@text.FillInBlanks" name="Cooking Terms" code="">
   <description />
   <messages>
    <cell type="initial">
     <style />
     <p>Please pick the right answer</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <container bgColor="0xFFFFFF">
     <counters time="false" actions="false" score="true" />
    </container>
    <window bgColor="0xFFFFFF" border="true">
     <size width="600" height="999" />
    </window>
